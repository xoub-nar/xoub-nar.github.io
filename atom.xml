    <link href="http://example.com/2021/07/08/%E8%B4%AA%E5%BF%83%E7%AE%97%E6%B3%95JAVA(%E4%B8%80)%EF%BC%9A/"/>
    <id>http://example.com/2021/07/08/%E8%B4%AA%E5%BF%83%E7%AE%97%E6%B3%95JAVA(%E4%B8%80)%EF%BC%9A/</id>
    <published>2021-07-08T12:13:11.150Z</published>
